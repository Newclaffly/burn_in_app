                                switch (data) {
                                    case 'PM EXPIRED':
                                        status = '<span class="badge bg-danger">' + data + '</span>';
                                        break;
                                    case 'PASSED':
                                        status = '<span class="badge bg-success">' + data + '</span>';
                                        break;
                                    case 'NOT PASSED':
                                        status = '<span class="badge bg-danger">' + data + '</span>';
