            function check_board_duplicate(para) {
                console.log(para);
                var temp = para;

                $.ajax({
                    type: "POST",
                    url: "./ajax_get_board_dupicate",
                    data: '',
                    datatype: "json",
                    success: function (data) {
                        var data_fillter_array = data.filter(function (entry) {
                            return entry.BI_BOARD_NO === temp;
                        });
                        //console.log(data_fillter_array);
                        if (data_fillter_array == '') {
                            //$("#btn_submit_add_board").removeAttr("disabled");
                            var add_board_master = {
                                "BI_BOARD_NO": $("#input_add_board_no").val(),
                                "BI_BOARD_MODEL": $("#input_add_board_model option:selected").val(),
                                "BI_BOARD_REGISTRATION_DATE": $("#input_add_board_regis_date").val(),
                                "BI_BOARD_LAST_PM": $("#input_add_last_pm").val(),
                                //"BI_BOARD_LIFE_TIME": $("#input_add_life_time").val(),
                                "LIFE_TIME": $("#input_add_life_time_condition").val(),
                                "BI_BOARD_STATUS": $("#input_add_board_stauts option:selected").val(),
                                "BI_BOARD_OPARATE": $("#input_add_board_op option:selected").val()
                            }

                            $.ajax({
                                type: "POST",
                                url: "./ajax_add_board_master",
                                data: { add_board_master: add_board_master },
                                datatype: "json",
                                success: function (data) {
                                    Swal.fire({
                                        icon: 'success',
                                        title: 'Adding',
                                        text: 'Your board master has been Adding.'
                                        //    footer: '<a href="">Why do I have this issue?</a>'
                                    })
                                    console.log(JSON.stringify(data));
                                    $('#table_bi_board_master').DataTable().clear();
                                    $('#table_bi_board_master').DataTable().ajax.reload();
                                    $('#modal_add_board_master').modal('hide');
                                },
                                error: function (request, status, error) {
                                    Swal.fire({
                                        icon: 'error',
                                        title: 'Oops...',
                                        text: 'Something went wrong!',
                                        footer: '<a href="">Why do I have this issue?</a>'
                                    })
                                }
                            })
                        } else {
                            //$("#btn_submit_add_board").attr("disabled", true);
                            Swal.fire({
                                icon: 'error',
                                title: 'Oops...',
                                text: 'Board No. duplicate!',
                                footer: '<a href="">Why do I have this issue?</a>'
                            })
                        }
