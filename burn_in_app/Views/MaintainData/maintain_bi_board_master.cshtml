            function create_table_board_master_list() {
                $('#table_bi_board_master').DataTable({
                    "destroy": true,
                    "searching": true,
                    "lengthChange": false,
                    "paging": false,
                    "bInfo": false,
                    "stateSave": false,
                    "scrollY": '70vh',
                    //"scrollX": true,
                    "scrollCollapse": true,
                    "ajax": {
                        "type": "POST",
                        "url": "./ajax_display_board_master",
                        "datatype": "json"
                    },
                    //createdRow: function (node, data) {
                    //    if (data.hasOwnProperty('BI_BOARD_STAUS_NAME')) {
                    //        if (data.BI_BOARD_STAUS_NAME == 'PASSED') {
                    //            $('td', node).eq(5).addClass('highligh_green');
                    //        }
                    //        if (data.BI_BOARD_STAUS_NAME == 'PM EXPIRED') {
                    //            $('td', node).eq(5).addClass('highligh_red');
                    //        }
                    //        if (data.BI_BOARD_STAUS_NAME == 'NOT PASSED') {
                    //            $('td', node).eq(5).addClass('highligh_red');
                    //        }
                    //    }
                    //},
                    columnDefs: [
                        { targets: [0, 1, 2, 3, 4, 5, 6, 7], className: 'text-center' },
                    ],
                    "columns": [
                        { data: "BI_BOARD_NO", "name": "BI_BOARD_NO", "autoWidth": true },
                        { data: "BI_BOARD_MODEL", "name": "BI_BOARD_MODEL", "autoWidth": true },
                        {
                            data: "BI_BOARD_LAST_PM",
                            "render": function (value) {
                                if (value === null) return "";
                                var date = eval(value.replace(/\/Date\((\d+)\)\//gi, "new Date($1)"));
                                return dayjs(date).format('YYYY-MM-DD');
                            }
                        },
                        {
                            data: "BI_BOARD_NEXT_PM",
                            "render": function (value) {
                                if (value === null) return "";
                                var date = eval(value.replace(/\/Date\((\d+)\)\//gi, "new Date($1)"));
                                return dayjs(date).format('YYYY-MM-DD');
                            }
