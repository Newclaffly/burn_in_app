                console.log(edit_board_master);
                $.ajax({
                    type: "POST",
                    url: "./ajax_edit_board_master",
                    data: { edit_board_master: edit_board_master },
                    datatype: "json",
                    success: function (data) {
                        Swal.fire({
                            icon: 'success',
