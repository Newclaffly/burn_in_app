                    success: function (data) {
                        var $select = $('#fillter_board_status');
                        $.each(data, function (i) {
                            $.each(data[i], function (key, val) {
                                $select.append('<option value=' + val.BI_BOARD_STATUS + '>' + val.BI_BOARD_STAUS_NAME + '</option>');
