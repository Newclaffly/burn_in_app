                $.ajax({
                    type: "POST",
                    url: "./ajax_get_board_model_master",
                    datatype: "json",
                    success: function (data) {
                        var $select = $('#fillter_board_model');
                        $.each(data, function (i) {
                            $.each(data[i], function (key, val) {
                                $select.append('<option value=' + val + '>' + val + '</option>');
