    <script>
        $(document).ready(function () {
            //test();
            //var table = $('#table_bi_board_master').DataTable();

            get_board_model();
            create_table_board_master_list();
            var table = $('#table_bi_board_master').DataTable();

            get_board_status();

            $('.datepicker').datepicker(
